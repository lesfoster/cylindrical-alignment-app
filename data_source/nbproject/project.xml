<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.j2ee.ejbjarproject</type>
    <configuration>
        <buildExtensions xmlns="http://www.netbeans.org/ns/ant-build-extender/1">
            <extension file="xml_binding_build.xml" id="jaxb">
                <dependency dependsOn="jaxb-code-generation" target="-pre-pre-compile"/>
            </extension>
        </buildExtensions>
        <data xmlns="http://www.netbeans.org/ns/j2ee-ejbjarproject/3">
            <name>data_source</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <included-library dirs="200">reference.utilities.jar</included-library>
            <included-library dirs="200">file.reference.httpclient-4.5.2.jar</included-library>
            <included-library dirs="200">file.reference.httpcore-4.4.4.jar</included-library>
            <included-library dirs="200">file.reference.commons-logging-1.2.jar</included-library>
            <included-library dirs="200">file.reference.kxml2-2.3.0.jar</included-library>
            <included-library dirs="200">file.reference.JSON-java.jar</included-library>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.java.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>utilities</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
    </configuration>
</project>
